#lalt - return : ~/bin/newterm.sh
lalt - return : open -na /Applications/iTerm.app && osascript $HOME/.iterm2_newwindow.scpt
lalt - e : open -na /Applications/Emacs.app
#lalt - d : open -na /Applications/Firefox.app
lalt - d : open -na /Applications/Google\ Chrome.app
lalt + shift - d : open -na /Applications/Chromium.app

lshift + lalt - r: launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"
lshift + lalt + lctrl - r: yabairestart.sh
#lshift + lalt - t: brew services reload cmacrae/formulae/spacebar
lshift + lalt - q: yabai -m window --toggle float
lshift + lalt - a: yabai -m config --space $(yabai -m query --spaces --space|\jq -r .index) layout $(yabai -m query --spaces --space |jq -r 'if .type|contains("float") then "bsp" else "float" end')
lshift + lalt - space: yabai -m space --balance
lshift + lalt - u: open -a Ãœbersicht.app 
lshift + lalt - l: landscape

lalt - f: yabai -m window --toggle zoom-fullscreen
lalt - s: yabai -m window --toggle topmost
lshift + lalt - f: yabai -m window --toggle native-fullscreen

lalt - left: yabai -m window --focus west
lalt - down: yabai -m window --focus south
lalt - up: yabai -m window --focus north
lalt - right: yabai -m window --focus east

lalt + lshift - s: yabai -m window --toggle split

lalt + lshift - left: yabai -m window --swap west
lalt + lshift - down: yabai -m window --swap south
lalt + lshift - up: yabai -m window --swap north
lalt + lshift - right: yabai -m window --swap east

lctrl + lshift + lalt - left: yabai -m window --warp west
lctrl + lshift + lalt - down: yabai -m window --warp south
lctrl + lshift + lalt - up: yabai -m window --warp north
lctrl + lshift + lalt - right: yabai -m window --warp east

lalt - v : yabai -m window --insert south
lalt - h : yabai -m window --insert east

lalt + lshift - 0x12: yabai -m window --space 1
lalt + lshift - 0x13: yabai -m window --space 2
lalt + lshift - 0x14: yabai -m window --space 3
lalt + lshift - 0x15: yabai -m window --space 4
lalt + lshift - 0x17: yabai -m window --space 5
lalt + lshift - 0x16: yabai -m window --space 6
lalt + lshift - 0x1A: yabai -m window --space 7
lalt + lshift - 0x1C: yabai -m window --space 8
lalt + lshift - 0x19: yabai -m window --space 9
lalt + lshift - 0x1D: yabai -m window --space 10

