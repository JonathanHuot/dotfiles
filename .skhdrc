alt - return : open -na /Applications/iTerm.app && osascript $HOME/.iterm2_newwindow.scpt
alt - e : open -na /Applications/Emacs.app
alt - d : open -na /Applications/Firefox.app

shift + alt - r: launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"
shift + lalt - space: yabai -m space --balance

alt - f: yabai -m window --toggle zoom-fullscreen
alt - s: yabai -m window --toggle topmost
shift + alt - f: yabai -m window --toggle native-fullscreen

alt - left: yabai -m window --focus west
alt - down: yabai -m window --focus south
alt - up: yabai -m window --focus north
alt - right: yabai -m window --focus east

alt + shift - s: yabai -m window --toggle split

alt + shift - left: yabai -m window --swap west
alt + shift - down: yabai -m window --swap south
alt + shift - up: yabai -m window --swap north
alt + shift - right: yabai -m window --swap east

ctrl + shift + alt - left: yabai -m window --warp west
ctrl + shift + alt - down: yabai -m window --warp south
ctrl + shift + alt - up: yabai -m window --warp north
ctrl + shift + alt - right: yabai -m window --warp east

alt - v : yabai -m window --insert south
alt - h : yabai -m window --insert east

alt + shift - 0x12: yabai -m window --space 1
alt + shift - 0x13: yabai -m window --space 2
alt + shift - 0x14: yabai -m window --space 3
alt + shift - 0x15: yabai -m window --space 4
alt + shift - 0x16: yabai -m window --space 5
alt + shift - 0x17: yabai -m window --space 6
alt + shift - 0x18: yabai -m window --space 7
alt + shift - 0x19: yabai -m window --space 8

alt - 0x12: yabai -m space --focus 1
alt - 0x13: yabai -m space --focus 2
alt - 0x14: yabai -m space --focus 3
alt - 0x15: yabai -m space --focus 4
alt - 0x16: yabai -m space --focus 5
alt - 0x17: yabai -m space --focus 6
alt - 0x18: yabai -m space --focus 7
alt - 0x19: yabai -m space --focus 8
